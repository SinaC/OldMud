    <!-- Menu deroulant permettant d'acceder aux differentes pages de statistique-->
    <script language="JavaScript">
      // Script trouvé sur Le Webmestre  www.le-webmestre.net   info@le-webmestre.net
      // Retrouvez des centaines de scripts et bien plus...
      function fonctionMenu(form) {
        var choix=form.menuchoix.selectedIndex //lwf
        if ( form.menuchoix.options[choix].value != "INVALID" ) {
          location=form.menuchoix.options[choix].value;
        }
      }
    </script>
